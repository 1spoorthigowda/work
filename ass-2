#include <stdio.h>

int main() {
    int num1, num2, num3, sum;

    // Taking input
    printf("Enter three numbers: ");
    scanf("%d %d %d", &num1, &num2, &num3);

    // Adding the numbers
    sum = num1 + num2 + num3;

    // Displaying the result
    printf("Sum = %d\n", sum);

    return 0;
}

